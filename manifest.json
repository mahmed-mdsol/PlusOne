{
  "manifest_version": 2,

  "name": "Plus One",
  "description": "This estension displys plus ones to its less popular form 0.9 repeating. ",
  "version": "1",

    "content_scripts": [
        {
            "matches": ["https://plus.google.com/*"],
            "js": ["jquery.min.js", "plus_one.js"]
        }
    ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
